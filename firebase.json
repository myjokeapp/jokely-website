{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/node_modules/**",
      "**/package.json",
      "**/package-lock.json",
      "**/webpack*"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|webp|css|js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      }
    ]
  }
}
